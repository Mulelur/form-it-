{
	"arrowParens": "always",
	"bracketSpacing": true,
	"endOfLine": "auto",
	"jsxBracketSameLine": false,
	"jsxSingleQuote": false,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"semi": true,
	"singleQuote": true,
	"printWidth": 80,
	"tabWidth": 2,
